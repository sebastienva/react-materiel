{
  "name": "react-materiel",
  "version": "0.0.8",
  "description": "Material implementation for react based on material-design-lite",
  "main": "./build/index.js",
  "scripts": {
    "start": "cd demo && npm start",
    "build": "webpack",
    "dev": "cd demo && npm run dev",
    "lint": "eslint src",
    "test": "jest",
    "coveralls": "jest --coverage && cat ./coverage/lcov.info | coveralls",
    "doc": "./node_modules/.bin/react-docgen src/ | ./doc/buildDocs.sh",
    "demo": "cd demo && npm run build && npm start",
    "npm-build": "babel ./src --out-dir ./build --ignore test.js && npm publish && rm -rf build",
    "heroku-postbuild": "cd demo && npm run postinstall"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sebastienva/materiel.git"
  },
  "nyc": {
    "sourceMap": false,
    "instrument": false
  },
  "keywords": [
    "react",
    "reactjs",
    "material"
  ],
  "author": "sv",
  "license": "MIT",
  "homepage": "https://github.com/sebastienva/materiel.git",
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-core": "^6.0.20",
    "babel-eslint": "^6.1.0",
    "babel-jest": "^16.0.0",
    "babel-loader": "^6.0.1",
    "babel-plugin-flow-react-proptypes": "^0.8.0",
    "babel-plugin-transform-flow-strip-types": "^6.8.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.0.15",
    "coveralls": "^2.11.9",
    "cross-env": "^2.0.0",
    "css-loader": "^0.23.1",
    "enzyme": "^2.5.1",
    "eslint": "^2.13.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.9.2",
    "eslint-plugin-jsx-a11y": "^1.5.3",
    "eslint-plugin-react": "^5.0",
    "file-loader": "^0.8.5",
    "flow-bin": "^0.26.0",
    "jest": "^16.0.2",
    "json-loader": "^0.5.4",
    "node-sass": "^3.7.0",
    "react": "^15.3.2",
    "react-addons-test-utils": "^15.3.2",
    "react-docgen": "^2.9.1",
    "react-dom": "^15.3.2",
    "react-hot-loader": "^1.3.0",
    "sass-loader": "^3.2.1",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.12.1"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "isomorphic-ensure": "^1.0.1",
    "material-design-lite": "^1.2.0",
    "react-ink": "^6.1.1",
    "react-onclickoutside": "^5.3.1",
    "react-router": "^2.4.1"
  }
}
